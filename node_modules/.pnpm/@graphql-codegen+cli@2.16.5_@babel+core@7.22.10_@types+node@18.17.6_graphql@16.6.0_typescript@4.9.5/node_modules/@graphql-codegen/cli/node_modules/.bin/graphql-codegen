#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules/@graphql-codegen/cli/cjs/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules/@graphql-codegen/cli/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules/@graphql-codegen/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules/@graphql-codegen/cli/cjs/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules/@graphql-codegen/cli/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules/@graphql-codegen/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/@graphql-codegen+cli@2.16.5_@babel+core@7.22.10_@types+node@18.17.6_graphql@16.6.0_typescript@4.9.5/node_modules:/Users/chrisma/Documents/nhost-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cjs/bin.js" "$@"
else
  exec node  "$basedir/../../cjs/bin.js" "$@"
fi
